{
  "$schema": "http://json-schema.org/schema",
  "id": "ngx-pointdeb-material",
  "title": "Material initializer",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "the project to initialize",
      "$default": {
        "$source": "projectName"
      }
    },
    "placeholder": {
      "type": "boolean",
      "default": false,
      "description": "preserve angular placeholder or not.",
      "$x-prompt": "Would you like to preserve angular placeholder (app.component.html)?"
    }
  },
  "required": []
}
